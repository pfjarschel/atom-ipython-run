{
  "name": "ipython-run",
  "main": "./lib/ipython-run",
  "version": "1.0.0",
  "description": "Run python file being edited in Atom on an ipython console running in a terminal session",
  "activationCommands": {
    "atom-text-editor": [
      "ipython-run:run-file",
      "ipython-run:open-terminal",
      "ipython-run:setwd"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pfjarschel/atom-ipython-run"
  },
  "keywords": [ "python", "ipython", "terminal", "shell", "run", "execute" ],
  "license": "MIT",
  "engines": {
    "atom": ">1.0.0"
  }
}
